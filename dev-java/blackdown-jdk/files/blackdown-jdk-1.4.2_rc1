VERSION="Blackdown JDK @PV@"
JAVA_HOME=/opt/@P@
CLASSPATH=/opt/@P@/jre/lib/rt.jar:.
JDK_HOME=/opt/@P@
JAVAC=${JAVA_HOME}/bin/javac
ADDPATH="${JAVA_HOME}/bin:${JAVA_HOME}/jre/bin"
ADDLDPATH="${JAVA_HOME}/jre/lib/@PLATFORM@/:${JAVA_HOME}/jre/lib/@PLATFORM@/native_threads/:${JAVA_HOME}/jre/lib/@PLATFORM@/classic/"
MANPATH=/opt/@P@/man
ENV_VARS="JAVA_HOME CLASSPATH JDK_HOME JAVAC ADDPATH ADDLDPATH MANPATH"
